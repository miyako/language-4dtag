        'include': '#fourd-comment'
      }
      {
        'include': '#fourd-declaration'
      }
      {
        'include': '#fourd-declaration-array'
      }
      {
        'include': '#fourd-command'
      }
      {
        'include': '#fourd-constant-string'
      }
      {
        'include': '#fourd-variable'
      }
