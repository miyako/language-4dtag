      }
      {
        'include': '#embedded-code'
      }
      {
        'include': '#unquoted-attribute'
      }
    ]
  'unquoted-attribute':
    'match': '(?<==)(?:[^\\s<>/\'"]|/(?!>))+'
    'name': 'string.unquoted.html'

  'fourd-tag':
    'patterns': [
      {
        'include': '#fourd-tag-without-formula'
      }
      {
        'include': '#fourd-tag-with-formula'
      }
      {
        'include': '#fourd-script'
      }
      {
        'include': '#fourd-dollar'
